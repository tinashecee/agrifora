    <script>
        // Add an event listener to the search input
        const searchInput = document.getElementById("search-input");
        searchInput.addEventListener("input", function () {
            filterTable();
        });
    
        // Function to filter the table based on user input
        function filterTable() {
            const input = searchInput.value.toLowerCase();
            const table = document.getElementById("delivery-notes-table");
            const rows = table.getElementsByTagName("tr");
    
            for (let i = 1; i < rows.length; i++) { // Start from 1 to skip the header row
                const row = rows[i];
                const deliveryNoteNumber = row.cells[0].textContent.toLowerCase();
                const unit = row.cells[1].textContent.toLowerCase();
                const date = row.cells[2].textContent.toLowerCase();
    
                if (
                    deliveryNoteNumber.includes(input) ||
                    unit.includes(input) ||
                    date.includes(input)
                ) {
                    row.style.display = "";
                } else {
                    row.style.display = "none";
                }
            }
        }
    </script>
<script>
    const deliveryNoteLinks = document.querySelectorAll('#delivery-notes-table tbody a');
    const pdfPreview = document.getElementById('pdf-preview');

    // Add click event listeners to delivery note links
    deliveryNoteLinks.forEach(link => {
        link.addEventListener('click', function (event) {
            event.preventDefault(); // Prevent the link from navigating to a new page
            const pdfUrl = this.getAttribute('href');
            pdfPreview.src = pdfUrl; // Load the PDF in the iframe
        });
    });
</script>
